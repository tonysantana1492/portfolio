{
  "functions": {
    "src/app/api/export/pdf/route.ts": {
      "maxDuration": 45,
      "memory": 1024
    }
  },
  "env": {
    "PUPPETEER_SKIP_CHROMIUM_DOWNLOAD": "true",
    "PUPPETEER_SKIP_DOWNLOAD": "true",
    "PUPPETEER_EXECUTABLE_PATH": "/tmp/chromium",
